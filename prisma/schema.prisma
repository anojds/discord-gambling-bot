generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

model User {
    id Int @id @default(autoincrement())
    name String @unique
    username String
    createdAt DateTime @default(now())
    money BigInt @default(0)
    betWin Int @default(0)
    betFailed Int @default(0)
    lastDaily String @default("")
}